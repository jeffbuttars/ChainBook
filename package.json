{
  "name": "chainbook",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-semistandard": "^12.0.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-react": "^7.5.1",
    "react-scripts": "1.0.17"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.1",
    "victory": "^0.24.5",
    "web3": "^1.0.0-beta.27"
  },
  "scripts": {
    "start": ". ./.env.sh && react-scripts start",
    "build": ". ./.env.sh && react-scripts build",
    "test": ". ./.env.sh && react-scripts test --env=jsdom",
    "eject": ". ./.env.sh && react-scripts eject",
    "lint": "eslint --ext .js --ext .jsx --ignore-pattern 'src/style/semantic/*' src server",
    "lint:watch": "esw --watch --ext .js --ext .jsx --ignore-pattern 'src/style/semantic/*' src server",
    "yrebuild": "yarn autoclean --force; rm -f .yarnclean; rm -fr node_modules/; yarn install"
  }
}
